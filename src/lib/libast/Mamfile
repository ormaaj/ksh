		done

		make strhash.o
			make hash/strhash.c
				prev hash/hashlib.h
			done
			exec - compile %{<} -Ihash
		done

		make strkey.o
			make hash/strkey.c
				makp include/hashkey.h
				prev include/ast.h
			done
			exec - compile %{<}
		done

		make strsum.o
			make hash/strsum.c
				prev hash/hashlib.h
			done
			exec - compile %{<} -Ihash
		done

		make stracmp.o
			make string/stracmp.c
				prev include/ccode.h
				prev include/ast.h
			done
			exec - compile %{<}
